[req]
default_bits = 4096
prompt = no
default_md = sha512
distinguished_name = dn
req_extensions = req_ext
x509_extensions = v3_req

[dn]
C = EU
ST = State
L = City
O = Organization
OU = VASTraefik
emailAddress = selfsigned@vas.internal
CN = *.vas.internal

[v3_req]
basicConstraints = CA:FALSE
keyUsage = digitalSignature, keyEncipherment
subjectAltName = @alt_names

[req_ext]
subjectAltName = @alt_names

[alt_names]
DNS.1 = *.vas.internal
DNS.2 = vas.internal
